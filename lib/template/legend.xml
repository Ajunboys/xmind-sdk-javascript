<legend visibility="{%= data.visibility %}">
    <marker-descriptions>
    {% helper.each(data.markerDescriptions, function(md){ %}
        <marker-description description="{%= md.description %}" marker-id="{%= md.id %}"/>
    {% }); %}
    </marker-descriptions>
    <position svg:x="{%= data.position.x %}" svg:y="{%= data.position.y %}"/>
</legend>
